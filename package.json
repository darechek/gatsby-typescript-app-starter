{
	"name": "gatsby-typescript-app-starter",
	"private": true,
	"description": "A simple typescript application starter to get up and developing quickly with Gatsby",
	"version": "0.1.0",
	"author": "Erko Bridee <erko.bridee@gmail.com>",
	"dependencies": {
		"css-vars-ponyfill": "^2.0.2",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-helmet": "^5.2.1",
		"react-intl": "^2.9.0",
		"react-redux": "^7.1.0",
		"redux": "^4.0.1",
		"redux-thunk": "^2.3.0",
		"whatwg-fetch": "^3.0.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.4.4",
		"@types/node": "^12.0.8",
		"@types/react": "^16.8.21",
		"@types/react-dom": "^16.8.4",
		"@types/react-helmet": "^5.0.8",
		"@types/react-intl": "^2.3.18",
		"@types/react-redux": "^7.1.0",
		"@typescript-eslint/eslint-plugin": "^1.10.2",
		"@typescript-eslint/parser": "^1.10.2",
		"babel-plugin-react-intl": "^3.3.3",
		"babel-preset-gatsby": "^0.2.0",
		"browserslist": "4.6.2",
		"caniuse-lite": "1.0.30000974",
		"eslint": "^5.16.0",
		"eslint-config-prettier": "^5.0.0",
		"eslint-plugin-prettier": "^3.1.0",
		"gatsby": "^2.9.11",
		"gatsby-image": "^2.1.4",
		"gatsby-plugin-manifest": "^2.1.1",
		"gatsby-plugin-offline": "^2.1.3",
		"gatsby-plugin-react-helmet": "^3.0.12",
		"gatsby-plugin-sass": "^2.0.13",
		"gatsby-plugin-sharp": "^2.1.9",
		"gatsby-plugin-typescript": "^2.0.15",
		"gatsby-source-filesystem": "^2.0.43",
		"gatsby-transformer-sharp": "^2.1.21",
		"node-sass": "^4.12.0",
		"npm-run-all": "^4.1.5",
		"postcss-preset-env": "^6.6.0",
		"prettier": "^1.18.2",
		"react-intl-translations-manager": "^5.0.3",
		"rimraf": "^2.6.3",
		"typescript": "^3.5.2"
	},
	"keywords": [
		"gatsby",
		"starter",
		"typescript"
	],
	"license": "MIT",
	"scripts": {
		"translations:clean-messages": "rimraf .i18nExtractedMessages/",
		"translations:extract": "TRANSLATIONS=true babel src --extensions '.ts,.tsx' --out-file .i18nExtractedMessages/compiled.js",
		"translations:runner": "node ./scripts/translation-runner.js",
		"translations": "npm-run-all -s translations:extract translations:runner",
		"format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
		"lint": "eslint 'src/**/*.{ts,tsx}'",
		"type-check": "tsc",
		"gatsby-clean": "gatsby clean",
		"gatsby-develop": "gatsby develop -H 0.0.0.0",
		"gatsby-build": "gatsby build",
		"gatsby-serve": "gatsby serve -H 0.0.0.0",
		"clean": "npm-run-all -p translations:clean-messages gatsby-clean",
		"check": "npm-run-all -s type-check lint",
		"develop": "npm-run-all -s check translations gatsby-develop",
		"start": "npm run develop",
		"build": "npm-run-all -s check clean translations gatsby-build ",
		"serve": "npm-run-all -s build gatsby-serve"
	},
	"browserslist": [
		">1%",
		"ie>=11",
		"iOS >= 9",
		"Android >= 4.4",
		"not op_mini all"
	]
}
